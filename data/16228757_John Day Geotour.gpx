      <groundspeak:name>Antelope Mountain Lookout</groundspeak:name>
      <groundspeak:placed_by>WR7X</groundspeak:placed_by>
      <groundspeak:owner id="102403">WR7X</groundspeak:owner>
      <groundspeak:type>Traditional Cache</groundspeak:type>
