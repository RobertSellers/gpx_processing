    <type>Geocache|Traditional Cache</type>
    <groundspeak:cache id="4096114" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>fishermen's catch</groundspeak:name>
      <groundspeak:placed_by>whitnec</groundspeak:placed_by>
      <groundspeak:owner id="7729343">whitnec</groundspeak:owner>
      <groundspeak:type>Traditional Cache</groundspeak:type>
