    <groundspeak:cache id="1218367" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Black Rock</groundspeak:name>
      <groundspeak:placed_by>velirotta</groundspeak:placed_by>
