    <groundspeak:cache id="1377105" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Rocky View Dam</groundspeak:name>
      <groundspeak:placed_by>Osage2</groundspeak:placed_by>
