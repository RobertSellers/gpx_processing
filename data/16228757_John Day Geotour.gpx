    <groundspeak:cache id="4930519" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Hinds Feet on High Places</groundspeak:name>
      <groundspeak:placed_by>KarLee'sMASK</groundspeak:placed_by>
      <groundspeak:owner id="9386520">KarLee'sMASK</groundspeak:owner>
